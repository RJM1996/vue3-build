{
  "name": "vue3-webpack",
  "version": "1.0.0",
  "description": "使用 Webpack 搭建 Vue3 项目",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack serve -c ./build/webpack.config.js",
    "build": "NODE_ENV=production webpack -c ./build/webpack.config.js",
    "dev:rollup": "NODE_ENV=development rollup -w -c ./build/rollup.config.js",
    "build:rollup": "NODE_ENV=production rollup -c ./build/rollup.config.js",
    "dev:vite": "vite --host --config ./build/vite.config.js",
    "build:vite": "vite build --config ./build/vite.config.js"
  },
  "keywords": [
    "vue3",
    "webpack"
  ],
  "author": "rjm",
  "license": "ISC",
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-html": "^1.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.1",
    "rollup": "^3.5.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "terser-webpack-plugin": "^5.3.6",
    "vite": "^3.2.4",
    "vue-loader": "^17.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  }
}